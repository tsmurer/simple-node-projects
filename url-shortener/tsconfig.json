{
  "compilerOptions": {
      "target": "es2022",
      "module": "commonjs",
      "outDir": "dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "noImplicitAny":  
true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "skipLibCheck":  
true
  }
}